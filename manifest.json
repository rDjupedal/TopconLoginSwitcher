{
  "manifest_version": 2,
  "name": "Topcon Login Switcher",
  "version": "1.3",
  "description": "Topcon login switcher",
  "browser_specific_settings": {
    "gecko": {
      "id": "rasmus.djupedal@gmail.com"
    }
  },
  "icons": {
    "48": "favicon.ico"
  },
  "permissions": [
    "cookies",
    "<all_urls>",
    "tabs",
    "storage"
  ],
  "browser_action": {
    "default_icon": "favicon.ico",
    "default_title": "Topcon login switcher",
    "default_popup": "popup/menu.html"
  },
  "options_ui": {
    "page": "options.html"
  }
}